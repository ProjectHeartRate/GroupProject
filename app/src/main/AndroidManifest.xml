<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.srmar.groupproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplicationData"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AthleteActivity"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity
            android:name=".AthleteRangeActivity"
            android:parentActivityName=".AthleteActivity"></activity>
        <activity
            android:name=".CoachActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".AthleteStepsActivity"
            android:parentActivityName=".AthleteActivity">

        </activity>
        <activity
            android:name=".AthleteHeartRateActivity"
            android:parentActivityName=".AthleteActivity"></activity>
        <activity
            android:name=".CreateAthleteActivity"
            android:parentActivityName=".CoachActivity">
        </activity>

    </application>

</manifest>