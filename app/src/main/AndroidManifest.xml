<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.srmar.groupproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AthleteActivity"></activity>
        <activity android:name=".AthleteRangeActivity"></activity>
        <activity android:name=".CoachActivity"></activity>
        <activity android:name=".AthleteStepsActivity"></activity>
        <activity android:name=".AthleteHeartRateActivity"></activity>
    </application>

</manifest>